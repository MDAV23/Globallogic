package com.globallogic.July20;
import java.lang.String;

public class MyString {
	public static void main(String[] args) {
		String str=new String("www.google.com/wikipedia");
		char ch;
		String st="";
		for(int i=0;i<str.length();i++) {
			ch=str.charAt(i);
			if(ch=='.'||ch==','||ch==' '||ch=='	'||ch=='/'||ch==NULL) {
				//StringBuilder br=new StringBuilder(st);
				//System.out.println(br.reverse());
				System.out.print(st);
				System.out.print(ch);
				st="";
			}
			else
			{
				st=ch+st;
			}
				
		}
	}

}
